<div class="container">
  
    <table>
      <%= @event.each do |event| %>
      <tr>
        <td>
          <%= event.date %>
        </td>
        <td>
          <%= event.description %>
        </td>
        <td>
          <%= event.requires_id %>
        </td>
        <td>
          <%= event.capacity %>
        </td>
        <td>
          <%= event.directions %>
        </td>
     </tr>
     <% end %>
   </table>



</div>
